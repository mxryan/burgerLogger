<div class="row">
  <div class="col-md-4">
    <h4>Add a burger!</h4>
    <input type="text" id="new-input">
    <button id="sub-btn">Submit!</button>
  </div>
  
  <div class="col-md-4">
    <h4>Waiting to be eaten:</h4>
    <ul id="to-eat-list">
      {{#each burgers}}
      {{#unless devoured}}

      <li id={{id}}>{{burger_name}}<button class="devour-btn btn btn-info btn-sm" value={{id}}>
        Devour!
      </button></li>
      

      {{/unless}}
      {{/each}}
    </ul>
  </div>
  <div class="col-md-4">
    <h4>Devoured</h4>
    <ul id="eaten-list">
      {{#each burgers}}
      {{#if devoured}}
      <li>{{burger_name}}</li>
      {{/if}}
      {{/each}}
    </ul>
  </div>
</div>
<script src="https://code.jquery.com/jquery-3.3.1.min.js">
</script>
<script src="/assets/js/script.js">
</script>